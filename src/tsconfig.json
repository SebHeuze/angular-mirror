{
    "compilerOptions": {
        "target": "es5",
        "outDir": "dist",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": ["es2015", "dom"],
        "baseUrl": ".",
        "paths": {
            "annyang": [
                "app/lib/voicerecognition/annyang"
            ]
        },
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true
    }
}